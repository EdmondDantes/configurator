{
    "name": "ifcastle/configurator-ini",
    "description": "Configurator component.",
    "type": "ifcastle-component",
    "license": "MIT",
    "keywords": ["ifcastle", "configurator", "ini"],
    "extra": {
        "ifcastle-installer": {
            "package": {
                "name": "configurator",
                "bootloaders": [
                    "IfCastle\\Configurator\\ConfigApplication"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\Configurator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\Configurator\\": "./tests"
        }
    },
    "require": {
        "ifcastle/di": "^0.6.7",
        "ifcastle/os-utilities": "^0.5.0",
        "ifcastle/service-manager": "^0.5.0",
        "ifcastle/application": "^0.6.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "amphp/php-cs-fixer-config": "^2.1"
    }
}
